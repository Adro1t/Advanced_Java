package multithreadingPackage;

class Good {
	void display() {
		for (int i = 1; i <= 5; i++) {
			System.out.println("Good");
			Thread.sleep(1000); //1 second
		}
	}
}

class Morning {
	void display() {
		for (int i = 1; i <= 5; i++) {
			System.out.println("Good");
		}
	}
}

public class ConceptOfMultithreading {

	public static void main(String[] args) {

	}

}
