package InheritancePackage;

class Fruit {
	public Fruit() {
		System.out.println("Very Good for health");
	}
	void Taste() {
		System.out.println("Tastes good");
	}
}

class Apple extends Fruit{
	
}

public class ConceptofInheritance {

	public static void main(String[] args) {
		new Apple();
	}

}
