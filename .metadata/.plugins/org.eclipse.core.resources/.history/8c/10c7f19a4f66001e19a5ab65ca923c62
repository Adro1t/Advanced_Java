package multithreadingPackage;

class CSITBook {
	int total_seats = 48;

	public void book(int request_seats) {
		if (total_seats >= request_seats) {
			System.out.println(request_seats + " are booked successfully.");
			total_seats = total_seats - request_seats;
		}
	}
}

